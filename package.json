{
  "name": "fitme",
  "version": "0.0.3",
  "description": "FitMe - Fitness management made easy",
  "author": "Tyler Knipfer <knipferrc@gmail.com>",
  "license": "GPL-3.0",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon server",
    "build":
      "cross-env NODE_ENV=production parcel build index.html --public-url /",
    "start:prod": "cross-env NODE_ENV=production node server",
    "precommit": "lint-staged",
    "format":
      "prettier --write ./*.{ts,tsx,json,js,css} ./**/*.{ts,tsx,json,js,css}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knipferrc/fitme.git"
  },
  "bugs": {
    "url": "https://github.com/knipferrc/fitme/issues"
  },
  "keywords": ["typescript", "graphql", "fitme", "react"],
  "dependencies": {
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "@types/webpack-env": "^1.13.3",
    "apollo-server-express": "^1.3.2",
    "autoprefixer": "^7.2.5",
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "final-form": "^4.0.3",
    "fit.css": "0.0.2",
    "graphql": "^0.12.3",
    "graphql-subscriptions": "^0.5.6",
    "graphql-tools": "^2.18.0",
    "helmet": "^3.9.0",
    "hpp": "^0.2.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-final-form": "^3.0.4",
    "react-router-dom": "^4.2.2",
    "subscriptions-transport-ws": "^0.9.5"
  },
  "devDependencies": {
    "cross-env": "^5.1.3",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "nodemon": "^1.14.11",
    "parcel-bundler": "^1.4.1",
    "prettier": "^1.10.2",
    "typescript": "^2.6.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,css}": ["prettier --write", "git add"]
  }
}
