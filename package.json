{
  "name": "fitme",
  "version": "0.0.3",
  "description": "FitMe - Fitness management made easy",
  "author": "Tyler Knipfer <knipferrc@gmail.com>",
  "license": "GPL-3.0",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --open --config config/webpack.dev.js",
    "build": "NODE_ENV=production webpack --config config/webpack.prod.js",
    "lint": "eslint .",
    "precommit": "lint-staged",
    "format": "prettier --write \"src/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knipferrc/fitme.git"
  },
  "bugs": {
    "url": "https://github.com/knipferrc/fitme/issues"
  },
  "keywords": [
    "fitness",
    "graphql",
    "fitme",
    "react"
  ],
  "dependencies": {
    "antd": "^3.1.4",
    "apollo-cache-inmemory": "^1.1.5",
    "apollo-client": "^2.2.0",
    "apollo-link-http": "^1.3.2",
    "apollo-link-ws": "^1.0.4",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.6.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.26.0",
    "styled-components": "^3.0.2",
    "subscriptions-transport-ws": "^0.9.5",
    "unistore": "https://github.com/developit/unistore"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.9",
    "eslint": "^4.16.0",
    "eslint-plugin-react": "^7.6.0",
    "file-loader": "^1.1.6",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.0",
    "prettier": "^1.10.2",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "git add"
    ]
  }
